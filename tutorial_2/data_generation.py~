#/usr/bin/env python3

'''
this generates some random data for use with SAS, with names for the different columns
'''

import csv

def main():
    names = ['ID', 'obs1', 'obs2'];
    data = [names];
    for i in range(30):
        data.append([int(random.random()*100)+1, random.random(), exp(random.random()*10)])
    with open('../testdata/data.xls','w') as myfile:
        writer = csv.writer(myfiles)
        for line in data:
            writer.writerow(line)

if __name__=="__main__":
    main()
